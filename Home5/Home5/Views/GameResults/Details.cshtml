@model Home5.Models.GameResult

@{
    ViewBag.Title = "Подробная информация";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Подробная информация</h2>

<div>
    <h4>Результат игры</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Результат игры
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Result)
        </dd>

        <dt>
            Первый игрок
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FirstPlayer)
        </dd>

        <dt>
            Второй игрок
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SecondPlayer)
        </dd>

        <dt>
            Количество ходов
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StepsCount)
        </dd>

        <dt>
            Дата игры
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GameTime)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Назад", "Index")
</p>
