@model IEnumerable<Home5.Models.GameResult>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstPlayer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondPlayer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StepsCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GameTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Result)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstPlayer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondPlayer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StepsCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GameTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
